<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:id="@android:id/background">
		<shape>
			<size android:height="3dp" />
			<corners android:radius="0dip" />
			<gradient
				android:startColor="#3affffff"
				android:centerColor="#3affffff"
				android:centerY="0.75"
				android:endColor="#3affffff"
				android:angle="45" />
		</shape>
	</item>

	<item android:id="@android:id/secondaryProgress">
		<clip>
			<shape>
				<size android:height="3dp" />
				<corners android:radius="0dip" />
				<gradient
					android:startColor="#3affffff"
					android:centerColor="#3affffff"
					android:centerY="0.75"
					android:endColor="#3affffff"
					android:angle="45" />
			</shape>
		</clip>
	</item>

	<item android:id="@android:id/progress">
		<clip>
			<shape>
				<size android:height="3dp" />
				<corners android:radius="0dip" />
				<gradient
					android:startColor="#5dffffff"
					android:centerColor="#5dffffff"
					android:centerY="0.75"
					android:endColor="#5dffffff"
					android:angle="45" />
			</shape>
		</clip>
	</item>

</layer-list>