<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ipc.IPCDemoActivity">

    <Button
        android:id="@+id/get_aidl_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get aidl service data"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/get_ashmem_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get ashmem service data"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/get_aidl_service" />

    <Button
        android:id="@+id/get_messenger_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get messager service data"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/get_ashmem_service" />

</androidx.constraintlayout.widget.ConstraintLayout>